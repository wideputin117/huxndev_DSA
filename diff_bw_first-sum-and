/**
 * 1 Find sum
 * Square of the sum
 * sum of square of each n number
 * find difference between the two
 */

function difference(n){
    let sum = 0;
    let absDiff = 0;
    let squareOfN = 0;

    for(let i=0; i< n;i++){
        sum += n-i;
        squareOfN += Math.pow(n-i,2)
    }
    sum *= sum;
    absDiff =  sum- squareOfN
    console.log(absDiff);
}

difference(3);